  "source": "pragma solidity 0.5.8;\n\ncontract Ownable {\n    address payable public owner;\n    \n    modifier onlyOwner() {\n        require(msg.sender == owner, \"Caller must be contract owner\");\n        _; // continue execution\n    }\n    \n    constructor() public {\n        owner = msg.sender;\n    }\n}",
